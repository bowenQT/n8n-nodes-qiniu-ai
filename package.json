    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "gulp": "^4.0.2",
        "n8n-workflow": "^1.0.0",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
